<!DOCTYPE html>
<html>
  <head>
  
    <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.19.2/axios.min.js"></script>
    <script>
    
    
    async function GetData(){
      
      var response= await axios('https://jsonplaceholder.typicode.com/users');
      var data1= response.data;
      
      for(i=0;data1[i].id<=5;i++){
        
          document.getElementById('getData').innerHTML += "</br>Id: " +data1[i].id+
          " Name:"+data1[i].name+ "  Username: "+data1[i].username+" Email:"+data1[i].email+
          " Street: "+data1[i].address.street+ "City: "+data1[i].address.city+
          " Zipcode: "+data1[i].address.zipcode;
      }
    }
    
    function HideData(){
     document.getElementById('getData').innerHTML ="";
    }
     </script>
    
  </head>
  <body>
     <input type=submit  onclick="GetData()" value=GetData> 
     <input type=submit  onclick="HideData()" value=HideData>
     <br>
     <div id=getData></div>
   </body>
</html>
